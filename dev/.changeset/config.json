{
  "$schema": "https://unpkg.com/@changesets/config@1.6.3/schema.json",
  "changelog": [
    "@changesets/changelog-github",
    { "repo": "wagmi-dev/abitype" }
  ],
  "commit": false,
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
