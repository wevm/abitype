{
  "name": "abitype",
  "description": "Strict TypeScript types for Ethereum ABIs",
  "license": "MIT",
  "version": "0.8.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wagmi-dev/abitype.git"
  },
  "author": "awkweb.eth",
  "ethereum": "wagmi-dev.eth",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/wagmi-dev"
    }
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "module": "./dist/config.mts",
      "default": "./dist/config.js"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "module": "./dist/test/index.mts",
      "default": "./dist/test/index.js"
    },
    "./zod": {
      "types": "./dist/zod/index.d.ts",
      "module": "./dist/zod/index.mts",
      "default": "./dist/zod/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/config",
    "/test",
    "/zod",
    "/dist"
  ],
  "peerDependencies": {
    "typescript": ">=4.9.4",
    "zod": "^3 >=3.19.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@ethersproject/abi": "^5.7.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "17.0.21",
    "@vitest/coverage-c8": "^0.24.3",
    "bun": "^0.5.7",
    "ethers": "^6.0.5",
    "fs-extra": "^11.1.0",
    "lint-staged": "^12.4.1",
    "rome": "^12.0.0",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.4",
    "vitest": "^0.26.3",
    "zod": "^3.20.6"
  },
  "keywords": [
    "abi",
    "eth",
    "ethereum",
    "typescript",
    "web3"
  ],
  "scripts": {
    "bench": "vitest bench --no-threads",
    "build": "tsup",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "clean": "rm -rf config dist node_modules test trace zod **/node_modules",
    "dev": "DEV=true tsup",
    "docs:dev": "pnpm -r --filter docs dev",
    "docs:build": "pnpm -r --filter docs build",
    "docs:preview": "pnpm -r --filter docs preview",
    "lint": "rome check .",
    "lint:fix": "pnpm lint --apply",
    "lint:format": "rome format . --write",
    "postinstall": "pnpm dev",
    "prepare": "npx simple-git-hooks",
    "prepublishOnly": "pnpm bun scripts/prepublishOnly.ts",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:typecheck": "vitest typecheck",
    "test:update": "vitest --update",
    "trace": "tsc --noEmit --generateTrace ./trace --incremental false --project playground/tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm lint:fix"
    ],
    "*.{json,md,mdx,yml}": [
      "pnpm lint:format"
    ]
  },
  "packageManager": "pnpm@7.5.0",
  "pnpm": {
    "overrides": {
      "remark-shiki-twoslash>shiki": "^0.14.1",
      "shiki-twoslash>shiki": "^0.14.1"
    },
    "patchedDependencies": {
      "shiki-twoslash@3.1.1": "patches/shiki-twoslash@3.1.1.patch"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "engines": {
    "pnpm": ">=7"
  }
}
