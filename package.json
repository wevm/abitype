{
  "name": "abitype",
  "description": "Strict TypeScript types for Ethereum ABIs",
  "license": "MIT",
  "version": "0.8.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wagmi-dev/abitype.git"
  },
  "author": "awkweb.eth",
  "ethereum": "wagmi-dev.eth",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/wagmi-dev"
    }
  ],
  "type": "module",
  "module": "./dist/esm/src/index.js",
  "main": "./dist/cjs/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "typings": "./dist/types/src/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    },
    "./config": {
      "types": "./dist/types/src/config.d.ts",
      "import": "./dist/esm/src/config.js",
      "require": "./dist/cjs/src/config.js"
    },
    "./test": {
      "types": "./dist/types/src/test/index.d.ts",
      "import": "./dist/esm/src/test/index.js",
      "require": "./dist/cjs/src/test/index.js"
    },
    "./zod": {
      "types": "./dist/types/src/zod/index.d.ts",
      "import": "./dist/esm/src/zod/index.mts",
      "require": "./dist/cjs/src/zod/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "config": ["./dist/types/src/config.d.ts"],
      "test": ["./dist/types/src/test/index.d.ts"],
      "zod": ["./dist/types/src/zod/index.d.ts"]
    }
  },
  "files": [
    "dist", "!dist/**/*.tsbuildinfo", "src/**/*.ts", "!src/**/*.test.ts",
    "!src/**/*.test-d.ts", "!src/**/*.bench.ts", "!src/_test/**/*"
  ],
  "peerDependencies": {
    "typescript": ">=5.0.4",
    "zod": "^3 >=3.19.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@ethersproject/abi": "^5.7.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "17.0.21",
    "@vitest/coverage-c8": "^0.24.3",
    "bun": "^0.5.7",
    "ethers": "^6.0.5",
    "fs-extra": "^11.1.0",
    "lint-staged": "^12.4.1",
    "rimraf": "^5.0.0",
    "rome": "^12.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "vitest": "^0.26.3",
    "zod": "^3.20.6"
  },
  "keywords": ["abi", "eth", "ethereum", "typescript", "web3"],
  "scripts": {
    "bench": "vitest bench --no-threads",
    "build": "pnpm run clean && pnpm run build:cjs && pnpm run build:esm && pnpm run build:types",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --outDir ./dist/cjs --removeComments --verbatimModuleSyntax false && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --outDir ./dist/esm --removeComments && echo > ./dist/esm/package.json '{\"type\":\"module\"}'",
    "build:types": "tsc --project tsconfig.build.json --module esnext --declarationDir ./dist/types --emitDeclarationOnly --declaration --declarationMap",
    "changeset": "changeset",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version && pnpm install --lockfile-only && pnpm bun scripts/updateVersion.ts",
    "clean": "rimraf dist",
    "docs:dev": "pnpm -r --filter docs dev",
    "docs:build": "pnpm -r --filter docs build",
    "docs:preview": "pnpm -r --filter docs preview",
    "lint": "rome check .",
    "lint:fix": "pnpm lint --apply",
    "lint:format": "rome format . --write",
    "prepare": "npx simple-git-hooks",
    "prepublishOnly": "pnpm bun scripts/prepublishOnly.ts",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:typecheck": "vitest typecheck",
    "test:update": "vitest --update",
    "trace": "tsc --noEmit --generateTrace ./trace --incremental false --project playground/tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": ["pnpm lint:fix"],
    "*.{json,md,mdx,yml}": ["pnpm lint:format"]
  },
  "packageManager": "pnpm@7.5.0",
  "pnpm": {
    "overrides": {
      "remark-shiki-twoslash>shiki": "^0.14.1",
      "shiki-twoslash>shiki": "^0.14.1"
    },
    "patchedDependencies": {
      "shiki-twoslash@3.1.1": "patches/shiki-twoslash@3.1.1.patch"
    },
    "peerDependencyRules": {
      "ignoreMissing": ["@algolia/client-search"]
    }
  },
  "engines": {
    "pnpm": ">=7"
  }
}
